{
  "name": "minecraft-lang",
  "displayName": "Minecraft .lang Colorizer",
  "description": "An extension which colorizes lang files.",
  "publisher": "adamraichu",
  "authors": "AdamRaichu",
  "author": {
    "name": "AdamRaichu"
  },
  "version": "1.0.0",
  "browser": "./extension.ts",
  "activationEvents": [
    "onLanguage:mc-lang"
  ],
  "engines": {
    "vscode": "^1.22.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "mc-lang",
        "aliases": [
          "lang"
        ],
        "extensions": [
          "lang"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "mc-lang",
        "scopeName": "source.mc-lang",
        "path": "./syntaxes/mc-lang.tmLanguage.json"
      }
    ]
  }
}