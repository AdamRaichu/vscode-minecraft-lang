{
  "name": "minecraft-lang",
  "displayName": "Minecraft .lang Colorizer",
  "description": "An extension which colorizes lang files.",
  "publisher": "adamraichu",
  "icon": "logo.png",
  "authors": "AdamRaichu",
  "author": {
    "name": "AdamRaichu"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "url": "https://github.com/AdamRaichu/vscode-minecraft-lang",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/AdamRaichu/vscode-minecraft-lang/issues"
  },
  "galleryBanner": {
    "color": "#1f2227",
    "theme": "dark"
  },
  "version": "2.2.0",
  "browser": "/extension.js",
  "main": "/main.js",
  "activationEvents": [
    "onLanguage:mc-lang",
    "onCommand:AdamRaichu.minecraftLang.addStylingToWorkspace"
  ],
  "engines": {
    "vscode": "^1.63.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "mc-lang",
        "aliases": [
          "lang"
        ],
        "extensions": [
          "lang"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "mc-lang",
        "scopeName": "source.mc-lang",
        "path": "./syntaxes/mc-lang.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "AdamRaichu.minecraftLang.addStylingToWorkspace",
        "title": "MC Lang: Add ยง format styling to workspace setings",
        "shortTitle": "Add ยง styling"
      }
    ]
  }
}